generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


enum Role {
  ADMIN
  OBSERVER
}

model User {
  id            String   @id @default(uuid())
  email         String   @unique
  password_hash String
  role          Role
  isActive      Boolean  @default(true)
  createdAt     DateTime @default(now())
}

